/*
使用 View 作为结尾的主要原因是 VO 结尾不好看，而且界面层会体现在用户处，语法美观需要考虑
*/

/** 工程
gen-include:TaskView[]
*/
entity ProjectView{
    /** gen-from:ContractBO.customerName */
    customerName String required,
    projName String required,
    projDescription String,
    createDate LocalDate required,
}

/** 任务 */
entity TaskView {
    taskName String required,
    taskDescription String,
    /** gen-field: MemberView.name[] */
    employeelist String,
}

relationship OneToMany{
/** 工程的任务 */
    ProjectView{task}
    to
    /** 任务隶属工程 */
    TaskView{project}
}

entity MemberView{
/** 成员名称 */
    name String required,
}

relationship ManyToMany{
    TaskView{employee}
    to
    MemberView{task}
}

dto * with mapstruct

paginate * with pager

service * with serviceImpl








